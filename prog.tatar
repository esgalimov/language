#Factorial

bashlau

    esh fact(x)
        bashlau
            ans = 1;
            i = 1;
            yozyrga(ans);

            tugyaryak (i - x)
                bashlau
                    i = i + 1;
                    ans = ans * i;
                    yozyrga(ans)
                tuktau
        tuktau;

    fact(6)

tuktau
